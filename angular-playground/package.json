{
  "name": "angular-playground",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve -o --configuration=de",
    "build": "ng build --localize",
    "extract translations": "ng extract-i18n --output-path src/locale --format=xlf2",
    "build and deploy to github": "ng build --configuration production --base-href https://CrainCH.github.io/Angular/angular-playground && ngh --dir dist/angular-playground",
    "npm update": "npm update",
    "update angular core": "ng update @angular/cli @angular/core",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ng help": "ng"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.4",
    "@angular/common": "^12.0.4",
    "@angular/compiler": "^12.0.4",
    "@angular/core": "^12.0.4",
    "@angular/forms": "^12.0.4",
    "@angular/platform-browser": "^12.0.4",
    "@angular/platform-browser-dynamic": "^12.0.4",
    "@angular/router": "^12.0.4",
    "@ng-select/ng-select": "^3.7.3",
    "core-js": "^2.6.12",
    "g": "^2.0.1",
    "libphonenumber-js": "^1.9.19",
    "rxjs": "~6.5.5",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.0.4",
    "@angular/cli": "^12.0.4",
    "@angular/compiler-cli": "^12.0.4",
    "@angular/language-service": "^12.0.4",
    "@angular/localize": "^12.0.4",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.9",
    "@types/node": "^12.20.15",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.2.4"
